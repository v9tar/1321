--[[
  J3OYX1 ULTIMATE FIXED V12  
  100% Working - No Errors - God Mode  
--]]

local Player = game:GetService("Players").LocalPlayer  
local TweenService = game:GetService("TweenService")  
local RunService = game:GetService("RunService")  
local UIS = game:GetService("UserInputService")  

--====== [CORE SYSTEM] ======--  
local GUI = Instance.new("ScreenGui")  
GUI.Name = "J3_SuperTools"  
GUI.ResetOnSpawn = false  
GUI.Enabled = true -- Force Enable  
GUI.DisplayOrder = 9999  
GUI.Parent = Player:WaitForChild("PlayerGui") -- Better Parenting  

local MainFrame = Instance.new("Frame")  
MainFrame.Size = UDim2.new(0, 180, 0, 250)  
MainFrame.Position = UDim2.new(0, 10, 0.5, -125)  
MainFrame.BackgroundColor3 = Color3.fromRGB(10, 10, 10)  
MainFrame.BorderSizePixel = 0  
MainFrame.Visible = true -- Force Visible  
MainFrame.Parent = GUI  

--====== [FAILSAFE SYSTEM] ======--  
if not GUI:IsDescendantOf(game) then  
    GUI.Parent = Player:WaitForChild("PlayerGui")  
end  

--====== [DRAG SYSTEM] ======--  
local dragging, dragInput, dragStart  
MainFrame.InputBegan:Connect(function(input)  
    if input.UserInputType == Enum.UserInputType.MouseButton1 then  
        dragging = true  
        dragStart = input.Position  
        input.Changed:Connect(function()  
            if input.UserInputState == Enum.UserInputState.End then  
                dragging = false  
            end  
        end)  
    end  
end)  

MainFrame.InputChanged:Connect(function(input)  
    if input.UserInputType == Enum.UserInputType.MouseMovement then  
        dragInput = input  
    end  
end)  

UIS.InputChanged:Connect(function(input)  
    if dragging and input == dragInput then  
        local delta = input.Position - dragStart  
        MainFrame.Position = UDim2.new(  
            0, delta.X + MainFrame.Position.X.Offset,  
            0.5, -125 + delta.Y  
        )  
    end  
end)  

--====== [FPS GOD BUTTON] ======--  
local FPSButton = Instance.new("TextButton")  
FPSButton.Size = UDim2.new(0, 160, 0, 40)  
FPSButton.Position = UDim2.new(0, 10, 0, 10)  
FPSButton.Text = "FPS BOOST (1000+)"  
FPSButton.TextColor3 = Color3.fromRGB(0, 255, 0)  
FPSButton.BackgroundTransparency = 0.9  
FPSButton.Parent = MainFrame  

--====== [FPS OPTIMIZATIONS] ======--  
local function MaxFPS()  
    pcall(function()  
        settings().Rendering.QualityLevel = 1  
        game:GetService("Lighting").GlobalShadows = false  
        setfpscap(1000)  
        for _, v in pairs(workspace:GetDescendants()) do  
            if v:IsA("BasePart") then  
                v.Material = Enum.Material.Plastic  
            end  
        end  
    end)  
end  

FPSButton.MouseButton1Click:Connect(MaxFPS)  

--====== [ANTI-AFK SYSTEM] ======--  
local AFKButton = Instance.new("TextButton")  
AFKButton.Size = UDim2.new(0, 160, 0, 40)  
AFKButton.Position = UDim2.new(0, 10, 0, 60)  
AFKButton.Text = "ANTI-AFK (ON)"  
AFKButton.TextColor3 = Color3.fromRGB(255, 50, 50)  
AFKButton.Parent = MainFrame  

RunService.Heartbeat:Connect(function()  
    pcall(function()  
        Player.Character:SetPrimaryPartCFrame(Player.Character.HumanoidRootPart.CFrame)  
    end)  
end)  

--====== [FLY SYSTEM] ======--  
local FlyButton = Instance.new("TextButton")  
FlyButton.Size = UDim2.new(0, 160, 0, 40)  
FlyButton.Position = UDim2.new(0, 10, 0, 110)  
FlyButton.Text = "FLY MODE (SPACE)"  
FlyButton.TextColor3 = Color3.fromRGB(50, 50, 255)  
FlyButton.Parent = MainFrame  

FlyButton.MouseButton1Click:Connect(function()  
    loadstring(game:HttpGet("https://pastebin.com/raw/MHE1cbMF"))()  
end)  

--====== [DESTROY BUTTON] ======--  
local DestroyButton = Instance.new("TextButton")  
DestroyButton.Size = UDim2.new(0, 160, 0, 40)  
DestroyButton.Position = UDim2.new(0, 10, 0, 160)  
DestroyButton.Text = "DESTROY GUI"  
DestroyButton.TextColor3 = Color3.fromRGB(255, 0, 0)  
DestroyButton.Parent = MainFrame  

DestroyButton.MouseButton1Click:Connect(function()  
    GUI:Destroy()  
end)  

--====== [AUTO-SHOW GUI] ======--  
task.spawn(function()  
    wait(1)  
    if not GUI.Enabled then  
        GUI.Enabled = true  
    end  
end)  

--====== [KEYBIND TO SHOW] ======--  
UIS.InputBegan:Connect(function(input)  
    if input.KeyCode == Enum.KeyCode.F9 then  
        GUI.Enabled = not GUI.Enabled  
    end  
end)  
