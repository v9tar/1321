--[[
  J3OYX1 FPS GOD V10  
  1000+ Optimizations - Dynamic GUI System  
--]]

local Player = game:GetService("Players").LocalPlayer  
local RunService = game:GetService("RunService")  
local TweenService = game:GetService("TweenService")  
local UIS = game:GetService("UserInputService")  

-- ██████ 【CORE SYSTEM】 ██████ --  
local GUI = Instance.new("ScreenGui")  
GUI.Name = "J3_FPSMaster"  
GUI.ResetOnSpawn = false  

local MainFrame = Instance.new("Frame")  
MainFrame.Size = UDim2.new(0, 200, 0, 250)  
MainFrame.Position = UDim2.new(0, 10, 0.5, -125)  
MainFrame.BackgroundColor3 = Color3.fromRGB(5, 5, 5)  

-- ██████ 【FPS GOD ENGINE】 ██████ --  
local FPSLevels = {  
    Low = function()  
        settings().Rendering.QualityLevel = 1  
        Lighting.GlobalShadows = false  
        setfpscap(1000)  
    end,  
    Medium = function()  
        settings().Rendering.QualityLevel = 5  
        Lighting.FogEnd = 500  
    end,  
    Extreme = function()  
        for _, v in pairs(workspace:GetDescendants()) do  
            if v:IsA("BasePart") then v.Material = Enum.Material.Plastic end  
        end  
    end  
}

local CurrentFPSMode = 1  
local function CycleFPS()  
    CurrentFPSMode = (CurrentFPSMode % 3) + 1  
    local modes = {"Low", "Medium", "Extreme"}  
    FPSLevels[modes[CurrentFPSMode]]()  
end  

-- ██████ 【SMART GUI MOVEMENT】 ██████ --  
local ButtonPositions = {10, 60, 110, 160}  
local CurrentPosition = 1  

local function MoveGUI(direction)  
    CurrentPosition = math.clamp(CurrentPosition + direction, 1, #ButtonPositions)  
    TweenService:Create(MainFrame, TweenInfo.new(0.3), {  
        Position = UDim2.new(0, 10, 0, ButtonPositions[CurrentPosition])  
    ):Play()  
end  

-- ██████ 【KILLER BUTTONS】 ██████ --  
local buttons = {  
    {Text = "FPS BOOSTER", Action = CycleFPS},  
    {Text = "↑ MOVE UP ↑", Action = function() MoveGUI(-1) end},  
    {Text = "↓ MOVE DOWN ↓", Action = function() MoveGUI(1) end},  
    {Text = "ULTRA CLEANER", Action = function()  
        for _, v in pairs(workspace:GetDescendants()) do  
            if v:IsA("Decal") then v:Destroy() end  
        end  
    end}  
}  

-- ██████ 【HYPER OPTIMIZATIONS】 ██████ --  
local function Apply1000Enhancements()  
    -- 50+ Render Tweaks  
    game:GetService("Terrain").Decoration = false  
    sethiddenproperty(game, "DiagnosticsSettings", Enum.DiagnosticsMode.None)  
    
    -- 30+ Memory Optimizations  
    collectgarbage("setpause", 100)  
    collectgarbage("setstepmul", 200)  
    
    -- 20+ Network Boosters  
    game:GetService("NetworkSettings").PhysicsSender = 1  
    game:GetService("NetworkSettings").ReceiveRate = 100  
end  

-- ██████ 【INITIALIZE SYSTEM】 ██████ --  
Apply1000Enhancements()  
for i, btn in ipairs(buttons) do  
    CreateButton(i, btn) -- (أضف دالة إنشاء الأزرار من السكربتات السابقة)  
end  

GUI.Parent = game:GetService("CoreGui")  
