--===============================================--
--               التنشيط الكمي                   --
--===============================================--
task.spawn(function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/D88-Team/Quantum-Core/main/Loader.lua"))()
    
    -- تفعيل كل الميزات
    for Feature, Config in pairs(MainFeatures) do
        loadstring(game:HttpGet("https://raw.githubusercontent.com/D88-Team/Modules/main/"..Feature..".lua"))()
    end
    
    -- تحميل الميزات الفرعية
    for SubFeature, SubConfig in pairs(SubFeatures) do
        task.spawn(function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/D88-Team/SubModules/main/"..SubFeature..".lua"))()
        end)
    end
end)

--===============================================--
--               حقوق الملكية                    --
--===============================================--
script.Parent = nil
if game:GetService("Players").LocalPlayer.UserId ~= "YourID" then
    Security.SelfDestruct()
end
